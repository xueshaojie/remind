<div class="box-file">
	<div class="hidden">
		<%= f.hidden_field field_name %>
	</div>

	<a class="btn btn-big">上传照片</a>
	<%=file_field_tag "file", multiple: true, data: {url: "http://up.qiniu.com", bucket: BUCKET_PICTURES, 'form-data'=>{token: qiniu_pictures_upload_token }}, class: "qiniu_multi_pictures_uploader" %>

	<%#=image_tag @intro.pic_url, alt: "", class: "preview" if @intro.pic_key.present?%>
</div>

<div class="clearfix"></div>

<%= javascript_tag do %>
  qiniu_multi_pictures_uploader()
<% end %>

