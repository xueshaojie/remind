<!doctype html>
<html class="member">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <meta name="keywords" content="<%= content_for?(:keywords) ? yield(:keywords) : "标途" %>" />
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "标途" %>" />
    <meta name="viewport" content="width=960" />
    <%= content_for?(:head) ? yield(:head) : '标途' %>
    <%= stylesheet_link_tag "sessions", media: "all" %>
    <%= javascript_include_tag "sessions" %>
    <script src="/com/js/vendor/modernizr-2.6.2.min.js"></script>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <!--[if lt IE 7]>
      <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <%= render 'application/header_member_v5' %>
    
    <%= content_for?(:content) ? yield(:content) : yield %>

    <script src="/com/js/vendor/jquery.anythingslider.min.js"></script>
    <script src="/com/js/vendor/affix.js"></script>
    <script src="/com/js/vendor/scrollspy.js"></script>
    <script src="/com/js/plugins.js"></script>
    <script src="/com/js/main.js"></script>
    <script>
      $("#change_verify_code").click(function() {
          $image_code = $('#image_code');
          $image_code.attr("src", $image_code.attr("src") + Math.random());
          $('#verify_code').val('').focus();
          return false;
       });
    </script>
    <%= yield :custom_js if content_for?(:custom_js) %>
  </body>
</html>